<div class="content_bottom" style="margin-top:2em">
  <p id="notice"><%= notice %></p>

  <h3 style="color:#7aae42"><i class="fa fa-money"></i> Money In</h3>
  <div class="col_2">
    <div style="overflow-x:scroll">

      <% columns = @transfers.column_names %>
      <table id="transactionsTable" class="table">
        <thead>
        <tr>

          <% columns.each do |c| %>
            <th><%= c %></th>
          <% end %>

        </tr>
        </thead>

        <tbody>
        <% @transfers.each do |ipn| %>
          <tr>
            <% columns.each do |c| %>
              <td><%=  ipn[c] rescue nil %></td>
            <% end %>

          </tr>
        <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>

<br>
