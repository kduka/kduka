<div class="content_bottom" style="margin-top:2em">


  <h3 style="color:#7aae42"><i class="fa fa-shopping-cart"></i> Stores</h3>
  <div class="col-md-12 span_3">
    <%= link_to  admins_allemails_path(format: "xls"), style:'text-decoration:none' do %>
      <button class="kd_buttons"><i class="fa fa-download"></i> Download Emails</button>
    <%end%><br>
    <div class="col_2">
      <% if !@feed.blank? %>
        <div style="overflow-x:scroll">
          <table id="myTable" class="table">
            <thead>
            <tr>
              <th>Store</th>
              <th>Email</th>
              <th>Feedback</th>
            </tr>
            </thead>
            <tbody>
            <% @feed.each do |f| %>
              <tr>
                <td><%= Store.find(f.store_id).name rescue 0 %></td>
                <td><%= Store.find(f.store_id).email rescue 0 %></td>
                <td><%= f.feedback %></td>
              </tr>
            <% end %>
            </tbody>
          </table>
        </div>

      <% else %>

        <p>You dont have any Feedback</p>

      <% end %>

      <br/>
      <br/>
    </div>
  </div>
</div>

<div class="clearfix"></div>
