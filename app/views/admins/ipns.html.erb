<div class="content_bottom" style="margin-top:2em">
<p id="notice"><%= notice %></p>

<h3 style="color:#7aae42"><i class="fa fa-money"></i> IPNs</h3>
<div class="col_2">
<% columns = @ipns.column_names %>
<div style="overflow-x:scroll">
<table id="ipnTable" class="table">
  <thead>
  <tr>
    <% columns.each do |c| %>
      <th><%= c %></th>
    <% end %>
  </tr>
  </thead>

  <tbody>
  <% @ipns.each do |ipn| %>
    <tr>
      <% columns.each do |c| %>
        <td><%=  ipn[c] %></td>
      <% end %>
    </tr>
  <% end %>
  </tbody>
</table>
</div>
</div>
<br>
</div>
